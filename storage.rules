rules_version = '2';

// Firebase Storage Security Rules
service firebase.storage {
  match /b/{bucket}/o {
    // Allow public read access to all files in Collections/Photos
    match /Collections/Photos/{allPaths=**} {
      allow read: if true;
    }
    
    // Allow public read access to all files (you can restrict this later)
    match /{allPaths=**} {
      allow read: if true;
    }
  }
}
